<p id="notice"><%= notice %></p>

<h1>Ryokutyas</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ryokutyas.each do |ryokutya| %>
      <tr>
        <td><%= link_to 'Show', ryokutya %></td>
        <td><%= link_to 'Edit', edit_ryokutya_path(ryokutya) %></td>
        <td><%= link_to 'Destroy', ryokutya, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ryokutya', new_ryokutya_path %>
